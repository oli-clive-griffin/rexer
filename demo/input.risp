(let
    (l (list 1 2 3))
    (reduce
        (fn (f list)
            (let
                (innerf (fn (ilist acc)
                            (if (emptyp ilist)
                                acc
                                (innerf
                                    (cdr ilist)
                                    (f acc (first ilist))))))
                (innerf list 0))))
    (reduce + l))
