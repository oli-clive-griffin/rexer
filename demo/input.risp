(fn (add a b) (+ a b))

(fn (fib n)
    (fn (inner a b i)
        (if (= i n)
        b 
        (inner b (+ a b) (+ 1 i))))
    (inner 1 1 0))

(fn (map f l)
    (if (empty? l)
        l
        (cons (f (car l)) (map f (cdr l)))))

(fn (range top)
    (fn (inner arr i)
        (if (< i 0)
            arr
            (inner (cons i arr) (- i 1))))
    (inner `() (- top 1)))

(print (map fib (range 10)))
